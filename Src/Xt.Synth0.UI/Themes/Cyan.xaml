<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Brush x:Key="RowEnabledKey">#40FFFF</Brush>
    <Brush x:Key="ForegroundKey">#00FFFF</Brush>
    <Brush x:Key="Foreground1Key">#40FFFF</Brush>
    <Brush x:Key="Foreground2Key">#00C0C0</Brush>
    <Brush x:Key="Foreground3Key">#008080</Brush>
    <Brush x:Key="Foreground4Key">#002020</Brush>
    <ImageBrush x:Key="BackgroundParamKey" ImageSource="Noise.png" Stretch="None" Opacity="0.25" AlignmentX="Left" AlignmentY="Bottom"/>

</ResourceDictionary>