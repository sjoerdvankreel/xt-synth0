<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Brush x:Key="RowEnabledKey">#FFFFFF</Brush>
    <Brush x:Key="ForegroundKey">#D0D0D0</Brush>
    <Brush x:Key="Foreground1Key">#FFFFFF</Brush>
    <Brush x:Key="Foreground2Key">#A0A0A0</Brush>
    <Brush x:Key="Foreground3Key">#808080</Brush>
    <Brush x:Key="Foreground4Key">#404040</Brush>
    <ImageBrush x:Key="BackgroundParamKey" ImageSource="Noise.png" Stretch="None" Opacity="0.8" AlignmentX="Right" AlignmentY="Top"/>

</ResourceDictionary>