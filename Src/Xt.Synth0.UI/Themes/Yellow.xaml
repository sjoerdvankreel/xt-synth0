<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Brush x:Key="RowEnabledKey">#FFFF40</Brush>
    <Brush x:Key="ForegroundKey">#FFFF00</Brush>
    <Brush x:Key="Foreground1Key">#FFFF40</Brush>
    <Brush x:Key="Foreground2Key">#C0C000</Brush>
    <Brush x:Key="Foreground3Key">#808000</Brush>
    <Brush x:Key="Foreground4Key">#404000</Brush>
    <ImageBrush x:Key="BackgroundParamKey" ImageSource="Yellow.png"/>

</ResourceDictionary>