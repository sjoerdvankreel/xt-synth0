<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Brush x:Key="RowEnabledKey">#A0A0FF</Brush>
    <Brush x:Key="ForegroundKey">#8080FF</Brush>
    <Brush x:Key="Foreground1Key">#A0A0FF</Brush>
    <Brush x:Key="Foreground2Key">#6060FF</Brush>
    <Brush x:Key="Foreground3Key">#404080</Brush>
    <Brush x:Key="Foreground4Key">#101020</Brush>
    <ImageBrush x:Key="BackgroundParamKey" ImageSource="Noise.png" Stretch="None" Opacity="0.25" AlignmentX="Left" AlignmentY="Top"/>

</ResourceDictionary>