<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Brush x:Key="RowEnabledKey">#FF00A0</Brush>
    <Brush x:Key="ForegroundKey">#FF0080</Brush>
    <Brush x:Key="Foreground1Key">#FF00A0</Brush>
    <Brush x:Key="Foreground2Key">#FF0060</Brush>
    <Brush x:Key="Foreground3Key">#800040</Brush>
    <Brush x:Key="Foreground4Key">#200010</Brush>
    <ImageBrush x:Key="BackgroundParamKey" ImageSource="Noise.png" Stretch="None" Opacity="0.25" AlignmentX="Center" AlignmentY="Center"/>

</ResourceDictionary>